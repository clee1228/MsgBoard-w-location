<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">#00009A</item>
        <item name="colorPrimaryDark">#000080</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="smallfont" parent="@style/Base.TextAppearance.AppCompat.Title">
        <item name="android:textSize">20sp</item>
    </style>




    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="actionMenu" parent="@android:style/TextAppearance.Widget.IconMenu.Item">
        <item name="android:actionMenuTextColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>

    </style>







</resources>
